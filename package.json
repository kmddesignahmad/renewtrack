{
  "name": "renewtrack",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "db:init": "wrangler d1 execute renewtrack_db --local --file=./migrations/schema.sql",
    "db:seed": "wrangler d1 execute renewtrack_db --local --file=./migrations/seed.sql",
    "db:init:remote": "wrangler d1 execute renewtrack_db --remote --file=./migrations/schema.sql",
    "db:seed:remote": "wrangler d1 execute renewtrack_db --remote --file=./migrations/seed.sql",
    "pages:dev": "wrangler pages dev dist --d1=DB=renewtrack_db --compatibility-date=2024-01-01"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240117.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "wrangler": "^3.22.1"
  }
}
